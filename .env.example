# Coinbase API credentials
COINBASE_API_KEY=organizations/your-org-id/apiKeys/your-key-id
COINBASE_API_SECRET=-----BEGIN EC PRIVATE KEY-----\nYOUR PRIVATE KEY\n-----END EC PRIVATE KEY-----\n

# Google Cloud settings
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account-key.json

# LLM settings
LLM_PROVIDER=vertex
LLM_MODEL=gemini-1.5-flash
LLM_LOCATION=us-central1

# Trading settings
TRADING_PAIRS=BTC-USD,ETH-USD
DECISION_INTERVAL_MINUTES=60
RISK_LEVEL=medium
SIMULATION_MODE=true

# Portfolio settings
PORTFOLIO_REBALANCE=true
MAX_TRADE_PERCENTAGE=25
TARGET_CRYPTO_ALLOCATION=80
TARGET_USD_ALLOCATION=20

# Dashboard settings
DASHBOARD_TRADE_HISTORY_LIMIT=10

# Web Server Sync Configuration
WEBSERVER_SYNC_ENABLED=true
WEBSERVER_SYNC_PATH=/var/www/html/crypto-bot

# Logging settings
LOG_LEVEL=INFO
LOG_FILE=logs/crypto_bot.log
