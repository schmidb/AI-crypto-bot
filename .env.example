# Coinbase API credentials
# Format: organizations/{org_id}/apiKeys/{key_id}
COINBASE_API_KEY=organizations/your-org-id/apiKeys/your-key-id
# Format: EC private key in PEM format
COINBASE_API_SECRET=-----BEGIN EC PRIVATE KEY-----\nYOUR PRIVATE KEY\n-----END EC PRIVATE KEY-----\n

# Google Cloud settings
GOOGLE_CLOUD_PROJECT=your-google-cloud-project
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/credentials.json

# LLM settings
LLM_PROVIDER=vertex
LLM_MODEL=text-bison@002
LLM_LOCATION=us-central1

# Trading settings
TRADING_PAIRS=BTC-USD,ETH-USD
DECISION_INTERVAL_MINUTES=60
MAX_INVESTMENT_PER_TRADE_USD=100
RISK_LEVEL=medium
SIMULATION_MODE=false

# Logging settings
LOG_LEVEL=INFO
LOG_FILE=logs/crypto_bot.log
